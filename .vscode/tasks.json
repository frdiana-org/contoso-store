      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/services/apps/Apps.Api/Apps.Api.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
